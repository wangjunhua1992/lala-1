    <van-icon bind:click="onSelect" class="van-rate__item" color="{{disabled?disabledColor:item?color:voidColor}}" data-index="{{index}}" name="{{item?'star':'star-o'}}" size="{{size}}px" wx:for="{{list}}" wx:key="index"></van-icon>
</view>
