page {
    background-color: #f4f4f4;
}

.container .dialog .dialog-mask {
    display: none;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 100;
    background: rgba(0,0,0,0.7);
}

.container .dialog.dialog-show .dialog-mask {
    display: block;
}

.container .dialog .dialog-container {
    position: fixed;
    top: 0;
    width: 100%;
    background: white;
    transform: translateY(-150%);
    transition: all 0.4s ease;
    z-index: 120;
}

.container .dialog-show .dialog-container {
    transform: translateY(0);
}

.container .dialog-show .dialog-container .button-tab {
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
    justify-content: space-between;
    font-size: 14px;
    padding: 11px 0px;
}

.container .dialog-show .dialog-container .button-tab view {
    text-align: center;
}

.container .dialog .dialog-container .wui-cell::after {
    border-top: 1rpx solid #e5e5e5;
}

.container .dialog .dialog-container .multiple {
    height: 260px;
}

.container .dialog .dialog-container .multiple scroll-view {
    height: 100%;
}

.container .dialog .dialog-container .multiple .wui-cell::after {
    left: 0;
    right: 0;
}

.container .dialog .dialog-container .multiple .wui-cell:last-child::after {
    border-top: 0;
}

.container .dialog .dialog-container .multiple .wui-cell.active {
    color: #ff2d4b;
}

.container .dialog .dialog-container .multiple .wui-cell.active .weui-cell__ft {
    display: block;
}

.container .dialog .dialog-container .multiple .weui-cell__ft {
    display: none;
}

.container .dialog .dialog-container .multiple .wui-cell-bd .icon {
    margin-right: 5px;
}

.container .dialog .dialog-container .filter {
    height: 370px;
}

.container .dialog .dialog-container .filter scroll-view {
    height: 100%;
}

.container .dialog .dialog-container .bottom-tab {
    position: relative;
}

.container .dialog .dialog-container .bottom-tab .clear {
    position: absolute;
    top: 50%;
    left: 15px;
    transform: translate(0,-50%);
    font-size: 12px;
    color: #848484;
}

.container .dialog .dialog-container .bottom-tab .finish {
    float: right;
    text-align: center;
    background-color: #ff2d4b;
    padding: 14px 0;
    font-size: 14px;
    width: 100px;
    color: #fff;
}

.container .dialog .dialog-container .bottom-tab .finish text {
    margin-left: 5px;
    padding: 2px 5px;
    border-radius: 100%;
    font-size: 10px;
    background-color: #333;
    color: white;
}

.container .dialog .dialog-container .filter .button-list {
    margin: 15px 15px 0 15px;
}

.container .dialog .dialog-container .filter .button-list .item {
    font-size: 14px;
    display: inline-block;
    width: 30%;
    text-align: center;
    border: 1px solid #F1EFF0;
    height: 35px;
    line-height: 35px;
    border-radius: 30px;
    margin-bottom: 15px;
    margin-right: 3.3%;
}

.container .dialog .dialog-container .filter .button-list .item:nth-child(3n+0) {
    margin-right: 0;
}

.container .dialog .dialog-container .filter .button-list .item.active {
    background-color: #FEFBF1;
    color: #ff2d4b;
    border-color: #ff2d4b;
}

.container .dialog .dialog-container .filter .button-list .item .icon {
    font-size: 17px;
    margin-right: 5px;
    color: #ff2d4b;
}

.container .dialog .dialog-container .filter .list-title {
    font-size: 14px;
    background-color: #FBFBFB;
    position: relative;
    height: 35px;
    line-height: 35px;
    padding-left: 15px;
}

.container .dialog .dialog-container .filter .list-title .line {
    height: 13px;
    width: 3px;
    background-color: #ff2d4b;
    position: absolute;
    top: 50%;
    left: 0;
    transform: translate(0,-50%);
}

.container .dialog .dialog-container .filter .active-list .icon-b {
    margin-top: 3px;
    width: 17px;
    height: 17px;
}

.container .dialog .dialog-container .filter .active-list .wui-cell::after {
    right: 0;
}

.container .dialog .dialog-container .filter .active-list .wui-cell:last-child::after {
    border-top: 0;
}

.container .dialog .dialog-container .filter .active-list .wui-cell .weui-cell__ft {
    display: none;
}

.container .dialog .dialog-container .filter .active-list .wui-cell.active .weui-cell__ft {
    display: block;
}

.container .dialog .dialog-container .filter .active-list .wui-cell.active {
    color: #ff2d4b;
}

.container .dialog .dialog-container .filter .active-list .tags::before {
    padding: 2px 4px;
}

.container .slide {
    height: 100px;
}

.container .slide image {
    display: inline-block;
    width: 100%;
    height: 100%;
}

.container .search-discount {
    margin-bottom: 10px;
    background: #fff;
}

.container .search-discount .col-60>view {
    padding: 11px;
    text-align: center;
}

.container .search-discount .col-60 .store-title {
    font-size: 17px;
    color: #fe2945;
}

.container .search-discount .col-60 .store-subtitle {
    color: #7b7b7b;
    font-size: 15px;
}

.container .search-discount .col-40>view {
    padding: 10px 17px 8px 0;
}

.container .search-discount .col-40 image {
    width: 48px;
    height: 48px;
    border-radius: 100%;
}

.slide .wx-swiper-dots.wx-swiper-dots-horizontal {
    margin-bottom: -2rpx;
}

.slide .wx-swiper-dot {
    width: 5px;
    display: inline-flex;
    height: 10rpx;
    margin-left: 5rpx;
    justify-content: space-between;
}

.slide .wx-swiper-dot::before {
    content: '';
    flex-grow: 1;
    background: rgba(255,255,255,0.8);
    border-radius: 100%;
}

.slide .wx-swiper-dot-active {
    width: 5px;
}

.slide .wx-swiper-dot-active::before {
    background: rgba(255,206,86,1);
}

.container .dialog .dialog-container .filter .new-search-group {
    display: flex;
    flex-wrap: wrap;
    position: relative;
    width: 100%;
    padding: 10px 12.5px;
    box-sizing: border-box;
}

.container .dialog .dialog-container .filter .new-search-group .new-search-item {
    width: 33.33%;
}

.container .dialog .dialog-container .filter .new-search-group .new-search-item .info {
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 14px;
    color: #333;
    padding: 8px 5px;
    background: #fafafa;
    margin: 0 2.5px 5px;
    box-sizing: border-box;
}

.container .dialog .dialog-container .filter .new-search-group .new-search-item .info.active {
    font-weight: 700;
    color: #3190e8;
    background-color: #edf5ff;
}

.container .dialog .dialog-container .filter .new-search-group .new-search-item .info .icon-waimai {
    font-size: 17px;
    margin-right: 5px;
    color: #FFD160;
}

.container .dialog .dialog-container .filter .new-search-group .new-search-item .info image {
    width: 17px;
    height: 17px;
    margin-right: 10px;
}

.container .dialog .dialog-container .search-btn {
    position: relative;
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
    justify-content: space-between;
    height: 43px;
}

.container .dialog .dialog-container .search-btn-left {
    font-size: 16px;
    text-align: center;
    flex: 1;
    background-color: #fff;
    color: #ddd;
    height: 43px;
    line-height: 43px;
}

.container .dialog .dialog-container .search-btn-right {
    font-size: 16px;
    text-align: center;
    flex: 1;
    background-color: #ff2d4b;
    color: #fff;
    height: 43px;
    line-height: 43px;
}

.container .cate-list-wrap {
    background-color: #fff;
    padding: 10px;
    box-sizing: border-box;
}

.container .cate-list-wrap .cate-list {
    width: 100%;
    white-space: nowrap;
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
}

.container .cate-list .cate-item {
    display: inline-flex;
    flex-flow: column nowrap;
    align-items: center;
    justify-content: flex-end;
    padding: 0 5px;
    max-width: 70px;
    height: 80px;
    box-sizing: border-box;
}

.container .cate-list .cate-item:first-child {
    padding-left: 0px;
}

.container .cate-list .cate-item:last-child {
    padding-right: 0px;
}

.container .cate-list .cate-item .img-wrap {
    width: 50px;
    height: 50px;
    font-size: 0px;
    border-radius: 50%;
    overflow: hidden;
    transition: all 200ms;
}

.container .cate-list .cate-item .cate-title {
    font-size: 14px;
    color: #000;
    width: 100%;
    text-align: center;
    margin-top: 5px;
    line-height: 1;
}

.container .cate-list .cate-item.active .img-wrap {
    width: 60px;
    height: 60px;
}

.container .cate-list .cate-item.active .cate-title {
    color: #ff2d4b;
    font-weight: 500;
}

.container .diy-store-other-condition .other-condition-item {
    line-height: 1;
    padding: 10px 0;
    text-align: center;
    background-color: #f5f5f5;
    border-radius: 3px;
}

.container .diy-store-other-condition .other-condition-item.active {
    background-color: rgba(255,45,75,0.1);
    color: #ff2d4b;
}

@import "../../nmy/7.wxss";