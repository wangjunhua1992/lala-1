    height: 54px;
    line-height: 50px;
    font-size: 16px;
    text-align: center;
    font-weight: bold;
}

.detail-content .cart .cart-btns .cart-btn-confirm .inner {
    background-color: #ff2d4b;
    color: #fff;
}

