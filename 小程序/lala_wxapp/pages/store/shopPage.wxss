.container .modal .wui-dialog-mask {
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.7);
    z-index: 203;
}

.container .modal .dialog-container .muti-dialog {
    z-index: 204;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    background-color: #fff;
    width: 90%;
}

.container .modal .dialog-container .muti-dialog .muti-close {
    position: absolute;
    top: 10px;
    right: 10px;
    width: 24px;
    height: 24px;
    text-align: center;
}

.container .modal .dialog-container .muti-dialog .muti-close::before,.container .modal .dialog-container .muti-dialog .muti-close::after {
    position: absolute;
    top: 10px;
    right: 1px;
    width: 20px;
    height: 1px;
    background: #666;
    content: '';
}

.container .modal .dialog-container .muti-dialog .muti-close::before {
    transform: rotate(45deg);
}

.container .modal .dialog-container .muti-dialog .muti-close::after {
    transform: rotate(135deg);
}

.container .modal .dialog-container .muti-dialog .muti-food-title {
    line-height: 44px;
    margin: 0 35px;
    font-size: 16px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    text-align: center;
    border-radius: 8px 8px 0 0;
}

.container .modal .dialog-container .muti-dialog .muti-cont {
    max-height: 250px;
    overflow: hidden;
    -webkit-box-flex: 1;
    font-size: 14px;
    padding-bottom: 10px;
}

.container .modal .dialog-container .muti-dialog .muti-cont .muti-sec {
    padding-top: 10px;
}

.container .modal .dialog-container .muti-dialog .muti-cont .muti-sec .muti-sec-title {
    line-height: 20px;
    margin: 0 10px;
}

.container .modal .dialog-container .muti-dialog .muti-cont .muti-sec .muti-choice {
    margin: 0 5px;
    overflow: hidden;
}

.container .modal .dialog-container .muti-dialog .muti-cont .muti-sec .muti-choice view {
    display: inline-block;
    float: left;
    margin: 10px 5px 0;
    min-width: 75px;
    padding: 0 10px;
    height: 28px;
    line-height: 26px;
    border: 1px solid #c4c4c4;
    box-sizing: border-box;
    border-radius: 14px;
    text-align: center;
    overflow: hidden;
}

.container .modal .dialog-container .muti-dialog .muti-cont .muti-sec .muti-choice view.selected {
    border-color: #ff2d4b;
    color: #ff2d4b;
    background: #fff;
}

.container .modal .dialog-container .muti-dialog .muti-choose {
    height: 56px;
    background-color: #f4f4f4;
    overflow: hidden;
    border-radius: 0 0 8px 8px;
}

.container .modal .dialog-container .muti-dialog .muti-choose .muti-oprt {
    float: right;
    display: inline-block;
}

.container .modal .dialog-container .muti-dialog .muti-choose .muti-cart-oprt {
