                    <view class="urltags-list">
                        <view bind:tap="onJsEvent" class="urltags-item" data-event-type="jsUrl" data-url="{{item.linkurl}}" wx:for="{{diyitem.data}}" wx:key="{{index}}">
                            <text class="urltags-text" style="color: {{item.color}}">{{item.text}}</text>
                        </view>
                    </view>
                </view>
            </view>
            <view class="diy-service" wx:elif="{{diyitem.id=='service'}}">
                <view class="diy-service-inner" wx:if="{{diyitem.params.servicefrom=='system'}}">
                    <view bind:tap="onJsEvent" class="wh-100" data-event-type="jsUrl" data-url="plugin/pages/kefu/chat?relation=member2kefu">
                        <image class="wh-100" src="{{diyitem.params.iconImg}}"></image>
                    </view>
                </view>
            </view>
            <view class="diy-hot-goods" wx:elif="{{diyitem.id=='hot_goods'}}">
                <view class="hot-goods-item" wx:for="{{10}}" wx:key="{{index}}">
                    <view class="goods-thumb">
                        <image alt="" class="img-100" src="http://testlala.oss-cn-qingdao.aliyuncs.com/images/1/2019/06/UBD99r24DG44v2ZO8dedX22XDHg2zV.jpg"></image>
                        <view class="label" style="color: {{diyitem.style.labelcolor}}; background-color: {{diyitem.style.labelbackground}}" wx:if="{{diyitem.params.showlabel==1}}">人气午餐</view>
                    </view>
                    <view class="goods-info" style="background-color: {{diyitem.style.background}}">
                        <view class="goods-title font-500 margin-5-t w-100 ellipsis" style="color: {{diyitem.style.goodstitlecolor}}">
							美式薯条
						</view>
                        <view class="font-12 flex-lr margin-10-t" style="color: {{diyitem.style.sailedcolor}}">
                            <text>月售169</text>
                            <text>40分钟</text>
                            <text>配送￥1</text>
                        </view>
                        <view class="flex margin-10-t">
                            <view class="c-danger flex" style="color: {{diyitem.style.pricecolor}}!important">
                                <text class="font-12">￥</text>
                                <text class="font-bold font-16">12.8</text>
                            </view>
                            <view class="font-12 c-gray line-through margin-5-l" style="color: {{diyitem.style.oldpricecolor}}" wx:if="{{diyitem.params.showoldprice==1}}">￥30</view>
                            <view class="discount" style="color: {{diyitem.style.pricecolor}}; border-color: {{diyitem.style.pricecolor}}" wx:if="{{diyitem.params.showdiscount==1}}">5折</view>
                        </view>
                        <view class="store-wrap van-hairline--top" wx:if="{{diyitem.params.showstore==1}}">
                            <view class="store-logo">
                                <image alt="" class="img-100" src="http://testlala.oss-cn-qingdao.aliyuncs.com/images/1/2019/06/OeFbZWfO4FWZ9Wtoa9oqExQAoofQm2.jpg"></image>
                            </view>
                            <view class="store-info">
                                <view class="store-title w-100 ellipsis" style="color: {{diyitem.style.storetitlecolor}}">商户名称商户名称商户名称</view>
                                <view class="flex font-12 c-gray" style="color: {{diyitem.style.storedeliverycolor}}">
                                    <text class="margin-5-r">4.6分</text>
                                    <text>起送￥20</text>
                                </view>
                            </view>
                        </view>
                    </view>
                </view>
            </view>
        </block>
    </view>
</template>
