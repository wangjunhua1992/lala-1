                                                    <view class="icon icon-favor_fill_light"></view>
                                                    <view class="icon icon-favor_fill_light"></view>
                                                    <view class="icon icon-favor_fill_light"></view>
                                                    <view class="icon icon-favor_fill_light"></view>
                                                </view>
                                                <view class="stars-bg" style="color: {{diyitem.style.scoreColor}};">
                                                    <view class="icon icon-favor_light"></view>
                                                    <view class="icon icon-favor_light"></view>
                                                    <view class="icon icon-favor_light"></view>
                                                    <view class="icon icon-favor_light"></view>
                                                    <view class="icon icon-favor_light"></view>
                                                </view>
                                            </view>
                                            <view class="store-sale">月售 {{stores.sailed}}</view>
