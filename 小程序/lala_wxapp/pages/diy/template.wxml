                                            <view class="discount-text">{{item.title}}</view>
                                        </view>
                                    </block>
                                    <view class="label-wrap {{stores.activity.is_show_all?'autoHeight':''}}" wx:if="{{diyitem.params.discountstyle==2}}">
                                        <text class="{{item.class}}" wx:if="{{index<4||stores.activity.is_show_all&&index>=4}}" wx:for="{{stores.activity.labels}}" wx:key="{{index}}">{{item.title}}</text>
                                    </view>
                                </view>
                                <view class="hot-box" wx:if="{{stores.hot_goods_num>0&&diyitem.params.showhotgoods==1}}">
                                    <navigator url="../store/goodsDetail?sid={{stores.store_id}}&id={{item.id}}" wx:for="{{stores.hot_goods}}" wx:key="{{index}}">
                                        <view class="hot-box-item">
                                            <view class="hot-t border-1px">
                                                <image mode="widthFix" src="{{item.thumb}}"></image>
                                                <view class="discount" wx:if="{{item.discount>0&&item.discount<10}}">{{item.svip_status==1?'会员':''}}{{item.discount}}折</view>
                                            </view>
                                            <view class="hot-b">
                                                <view class="shop-name">{{item.title}}</view>
                                                <view class="shop-price">
                                                    <view class="now-price">{{Lang.dollarSign}}{{item.price}}</view>
                                                    <view class="old-price" wx:if="{{item.old_price>0}}">{{Lang.dollarSign}}{{item.old_price}}</view>
                                                </view>
                                            </view>
                                        </view>
                                    </navigator>
                                </view>
