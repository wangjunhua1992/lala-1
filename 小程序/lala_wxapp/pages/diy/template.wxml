<template name="tpl-diy">
    <view class="diy-page container" scrollIntoView="{{scrollToId}}" style="background-color:{{diy.data.page.background}};">
        <view class="fixed-searchbar {{topSearchBar&&diy.is_has_location==1?'show':''}}" style="background-color: {{diy.fixedsearch.style.fixedbackground=='#ffffff'?'#ff2d4b':diy.fixedsearch.style.fixedbackground}}">
            <navigator url="{{diy.fixedsearch.params.link}}">{{diy.fixedsearch.params.linkto==1?'搜索信息内容':'搜索商家、商品'}}</navigator>
        </view>
        <block wx:for="{{diy.data.items}}" wx:for-index="diyindex" wx:for-item="diyitem" wx:key="{{diyindex}}">
            <block wx:if="{{diyitem.id=='danmu'}}">
                <swiper autoplay="true" circular="true" class="swiper-danmu {{diyitem.params.styleType==2?'style2':''}}" duration="0" interval="5000" style="opacity:{{danmu?diyitem.style.opacity:0}}; top: {{diyitem.style.top}}px; left: {{diyitem.style.left}}px;" vertical="true" wx:if="{{diyitem.params.status==1}}">
                    <swiper-item class="order-danmu " style="background: {{diyitem.style.background}};color:{{diyitem.style.color}};" wx:for="{{diyitem.members}}" wx:key="{{index}}">
                        <image class="thumb" src="{{item.avatar}}"></image>
                        <text class="info">新订单来自 {{item.nickname}}</text>
                        <text class="time">{{item.time}}</text>
                    </swiper-item>
                </swiper>
            </block>
            <view class="diy-richtext" style="background:{{diyitem.style.background}}; padding:{{diyitem.style.paddingtop}}px {{diyitem.style.paddingleft}}px;" wx:elif="{{diyitem.id=='richtext'}}">
                <template is="wxParse" data="{{wxParseData:richtext[diyindex].nodes}}"></template>
            </view>
            <block wx:elif="{{diyitem.id=='waimai_goods'}}">
                <view class="diy-waimai-food-list-onerow-box" style="background-color: {{diyitem.style.background}}; padding: {{diyitem.style.paddingtop}}px {{diyitem.style.paddingleft}}px;" wx:if="{{diyitem.style.liststyle=='1'}}">
                    <view class="item-list">
                        <view class="goods-item border-1px-t" wx:for="{{diyitem.data}}" wx:for-item="goodsitem" wx:key="{{index}}">
                            <view class="row item-wrapper">
                                <view class="col-40 goods-img">
                                    <image src="{{goodsitem.thumb}}"></image>
                                    <view class="discount" wx:if="{{diyitem.params.showicon==1&&goodsitem.discount>0&&goodsitem.discount<10}}">{{goodsitem.svip_status==1?'会员':''}}{{goodsitem.discount}}折</view>
                                </view>
                                <view class="col-60">
                                    <view class="goods-name">{{goodsitem.title}}</view>
                                    <view class="sale-num">已售:{{goodsitem.sailed}} 好评率:{{goodsitem.comment_good_percent}}</view>
                                    <view class="store-name">
                                        <view class="icon icon-shop"></view> {{goodsitem.store_title}}
										</view>
                                    <view class="price-buybtn">
                                        <view class="price-wrap">
                                            <view class="in-b now-price">{{Lang.dollarSign}}{{goodsitem.price}}</view>
                                            <view class="in-b old-price" style="color: {{diyitem.style.oldpricecolor}}" wx:if="{{diyitem.params.showoldprice==1}}">{{Lang.dollarSign}}{{goodsitem.old_price}}</view>
                                        </view>
                                        <navigator class="in-b buy-btn" style="background-color: {{diyitem.style.buybtncolor}}" url="../store/goodsDetail?sid={{goodsitem.sid}}&id={{goodsitem.goods_id}}">{{diyitem.params.buybtntext}}</navigator>
                                    </view>
                                </view>
                            </view>
                        </view>
                    </view>
                </view>
                <view class="diy-waimai-goods-list-box" style="background-color: {{diyitem.style.background}}; padding: {{diyitem.style.paddingtop}}px {{diyitem.style.paddingleft}}px;" wx:elif="{{diyitem.style.liststyle=='2'}}">
                    <view class="box-list">
                        <view class="box-item" wx:for="{{diyitem.data}}" wx:for-item="goodsitem" wx:key="{{index}}">
                            <view class="main-content-box">
                                <view class="goods-image" mode="widthFix">
                                    <image src="{{goodsitem.thumb}}"></image>
                                    <view class="discount" wx:if="{{diyitem.params.showicon==1&&goodsitem.discount>0&&goodsitem.discount<10}}">{{goodsitem.svip_status==1?'会员':''}}{{goodsitem.discount}}折</view>
                                </view>
                                <view class="goods-name">{{goodsitem.title}}</view>
                                <view class="discount-soldnum">
                                    <view class="in-b sold-num">已售{{goodsitem.sailed}}</view>
                                    <view class="in-b praise">好评率 {{goodsitem.comment_good_percent?goodsitem.comment_good_percent:'暂无'}}</view>
                                </view>
                                <view class="store-name">
                                    <view class="icon icon-shop"></view> {{goodsitem.store_title}}
									</view>
                                <view class="price-buybtn">
                                    <view class="in-b price">
                                        <view class="now-price">{{Lang.dollarSign}}{{goodsitem.price}}</view>
                                        <view class="old-price" style="color: {{diyitem.style.oldpricecolor}}" wx:if="{{diyitem.params.showoldprice==1}}">{{Lang.dollarSign}}{{goodsitem.old_price}}</view>
                                    </view>
                                    <navigator class="in-b buy-btn" style="background-color: {{diyitem.style.buybtncolor}}" url="../store/goodsDetail?sid={{goodsitem.sid}}&id={{goodsitem.goods_id}}">{{diyitem.params.buybtntext}}</navigator>
                                </view>
                            </view>
                        </view>
                        <view style="clear:both"></view>
                    </view>
                </view>
                <view class="diy-waimai-goods-list-threerow" style="background-color: {{diyitem.style.background}}; padding: {{diyitem.style.paddingtop}}px {{diyitem.style.paddingleft}}px;" wx:elif="{{diyitem.style.liststyle=='3'}}">
                    <scroll-view class="goods-list" scrollX="{{true}}">
                        <view bind:tap="onJsEvent" class="goods-item" data-event-type="jsUrl" data-url="pages/store/goodsDetail?sid={{goodsitem.sid}}&id={{goodsitem.goods_id}}" wx:for="{{diyitem.data}}" wx:for-item="goodsitem" wx:key="{{index}}">
                            <view class="item-inner">
                                <view class="thumb">
                                    <image src="{{goodsitem.thumb}}"></image>
                                    <view class="discount" wx:if="{{diyitem.params.showicon==1&&goodsitem.discount>0&&goodsitem.discount<10}}">{{goodsitem.svip_status==1?'会员':''}}{{goodsitem.discount}}折</view>
                                </view>
                                <view class="goods-title" style="color: {{diyitem.style.titlecolor}}">{{goodsitem.title}}</view>
                                <view class="flex store-info" wx:if="{{diyitem.params.storeshow==2&&goodsitem.store}}">
                                    <view class="store-title">{{goodsitem.store.title}}</view>
                                    <view class="delivery-time">{{goodsitem.store.delivery_time}}分钟</view>
                                </view>
                                <view class="goods-price">
                                    <view class="now-price" style="color: {{diyitem.style.pricecolor}}">{{Lang.dollarSign}}{{goodsitem.price}}</view>
                                    <view class="old-price" style="color: {{diyitem.style.oldpricecolor}}" wx:if="{{diyitem.params.showoldprice==1}}">{{Lang.dollarSign}}{{goodsitem.old_price}}</view>
                                </view>
                            </view>
                        </view>
                    </scroll-view>
                </view>
            </block>
            <view class="diy-waimai-store-box" style="background-color: {{diyitem.style.background}}; padding: {{diyitem.style.paddingtop}}px {{diyitem.style.paddingleft}}px;" wx:elif="{{diyitem.id=='waimai_stores'}}">
                <view class="waimai-store-item-list">
                    <view class="waimai-store-item border-1px-b {{store.is_rest==1?'disabled':''}}" wx:for="{{diyitem.data}}" wx:for-item="store" wx:key="{{store.id}}">
                        <view class="mian-content-box">
                            <view class="content-left border-1px">
                                <navigator class="item-image" url="{{store.url}}">
                                    <view class="item-rest" wx:if="{{store.is_rest==1}}">休息中</view>
                                    <image src="{{store.logo}}"></image>
                                </navigator>
                            </view>
                            <view class="content-right">
                                <navigator url="{{store.url}}">
                                    <view class="item-name-wrap">
                                        <view class="item-name" style="color: {{diyitem.style.titlecolor}}">{{store.title}}</view>
                                    </view>
                                    <view class="delivery-tip" wx:if="{{store.rest_reserve_cn}}">
                                        <text>{{store.rest_reserve_cn}}</text>
                                    </view>
                                    <view class="item-score-time">
                                        <view class="item-score-sale">
                                            <view class="item-star-box">
                                                <view class="stars" style="color: {{diyitem.style.scorecolor}}; width: {{store.score*20}}%">
                                                    <view class="icon icon-favor_fill_light"></view>
                                                    <view class="icon icon-favor_fill_light"></view>
                                                    <view class="icon icon-favor_fill_light"></view>
                                                    <view class="icon icon-favor_fill_light"></view>
                                                    <view class="icon icon-favor_fill_light"></view>
                                                </view>
                                                <view class="stars-bg" style="color: {{diyitem.style.scorecolor}};">
                                                    <view class="icon icon-favor_light"></view>
                                                    <view class="icon icon-favor_light"></view>
                                                    <view class="icon icon-favor_light"></view>
                                                    <view class="icon icon-favor_light"></view>
                                                    <view class="icon icon-favor_light"></view>
                                                </view>
                                            </view>
                                            <view class="item-sale">月售 {{store.sailed}}</view>
                                        </view>
                                        <view class="time-distance">
                                            <view class="avg_delivery_time">{{store.delivery_time}}分钟</view>
                                        </view>
                                    </view>
                                    <view class="item-min-delivery">
                                        <view class="item-min-delivery-left">
                                            <view>起送价{{Lang.dollarSign}}{{store.send_price}}</view>
                                            <view class="line">|</view>
                                            <view wx:if="{{store.delivery_price>0}}">配送费{{Lang.dollarSign}}{{store.delivery_price}}</view>
                                            <view wx:else>免配送费</view>
                                        </view>
                                        <view class="item-min-delivery-right-wrap">
                                            <view class="zhunshibao" wx:if="{{store.zhunshibao_cn}}">{{store.zhunshibao_cn}}</view>
                                            <view class="item-min-delivery-right" style="background-color: {{diyitem.style.deliverytitlebgcolor}}; color: {{diyitem.style.deliverytitlecolor}}" wx:if="{{store.delivery_title}}">{{store.delivery_title}}</view>
                                        </view>
                                    </view>
                                </navigator>
                                <view bind:tap="onToggleDiscount" class="discount-box" data-diyindex="{{diyindex}}" data-index="{{index}}" data-type="waimai_stores" wx:if="{{store.activity&&(store.activity.num>0||store.activity.labels_num>0)}}">
                                    <view class="toggle">
                                        <view class="icon {{store.activity.is_show_all?'icon-fold':'icon-unfold'}}" wx:if="{{diyitem.params.discountstyle==1&&store.activity.num>2||diyitem.params.discountstyle==2&&store.activity.labels_num>4}}"></view>
                                    </view>
                                    <block wx:if="{{diyitem.params.discountstyle==1}}" wx:for="{{store.activity.items}}" wx:key="{{index}}">
                                        <view class="single-line" wx:if="{{index<2||store.activity.is_show_all&&index>=2}}">
                                            <image class="discount-icon" src="/static/img/{{item.type}}_b.png"></image>
                                            <view class="discount-text">{{item.title}}</view>
                                        </view>
                                    </block>
                                    <view class="label-wrap {{store.activity.is_show_all?'autoHeight':''}}" wx:if="{{diyitem.params.discountstyle==2}}">
                                        <text class="{{item.class}}" wx:if="{{index<4||store.activity.is_show_all&&index>=4}}" wx:for="{{store.activity.labels}}" wx:key="{{index}}">{{item.title}}</text>
                                    </view>
                                </view>
                                <view class="hot-box" wx:if="{{store.hot_goods_num>0&&diyitem.params.showhotgoods==1}}">
                                    <navigator url="../store/goodsDetail?sid={{store.store_id}}&id={{item.id}}" wx:for="{{store.hot_goods}}" wx:key="{{index}}">
                                        <view class="hot-box-item">
                                            <view class="hot-t border-1px">
                                                <image mode="widthFix" src="{{item.thumb}}"></image>
                                                <view class="discount" wx:if="{{item.discount>0&&item.discount<10}}">{{item.svip_status==1?'会员':''}}{{item.discount}}折</view>
                                            </view>
                                            <view class="hot-b">
                                                <view class="shop-name">{{item.title}}</view>
                                                <view class="shop-price">
                                                    <view class="now-price">{{Lang.dollarSign}}{{item.price}}</view>
                                                    <view class="old-price" wx:if="{{item.old_price>0}}">{{Lang.dollarSign}}{{item.old_price}}</view>
                                                </view>
                                            </view>
                                        </view>
                                    </navigator>
                                </view>
                            </view>
                        </view>
                    </view>
                </view>
            </view>
            <view class="diy-notice" style="background-color: {{diyitem.style.background}}; padding: {{diyitem.style.paddingtop}}px {{diyitem.style.paddingleft}}px;" wx:elif="{{diyitem.id=='notice'&&diyitem.data}}">
                <view class="diy-notice-container border-1px-tb">
                    <view class="image border-1px-r">
                        <image alt="" src="{{diyitem.params.imgurl}}"></image>
                    </view>
                    <view class="icon">
                        <view class="icon icon-notification" style="color: {{diyitem.style.iconcolor}}"></view>
                    </view>
                    <view class="notice-text">
                        <swiper autoplay="true" circular="true" style="color: {{diyitem.style.textcolor}}" vertical="true">
                            <swiper-item wx:for="{{diyitem.data}}" wx:key="{{index}}">
                                <view class="swiper-slide swiper-slide-prev">
                                    <view bind:tap="onJsEvent" data-event-type="jsUrl" data-url="{{item.linkurl}}" wx:if="{{item.linkurl}}">{{item.title}}</view>
                                    <view bind:tap="onJsEvent" data-event-type="jsUrl" data-url="pages/public/richtext?key=notice&noticeid={{item.id}}" wx:else>{{item.title}}</view>
                                </view>
                            </swiper-item>
                        </swiper>
                    </view>
                    <view class="icon icon-arrow-right"></view>
                </view>
            </view>
            <view class="diy-img-card" style="padding: {{diyitem.style.paddingtop}}px {{diyitem.style.paddingleft}}px; background: {{diyitem.style.background}};" wx:elif="{{diyitem.id=='img_card'}}">
                <image alt="" mode="widthFix" src="{{diyitem.params.imgurl}}"></image>
            </view>
            <view class="diy-banner" style="padding: {{diyitem.style.paddingtop}}px {{diyitem.style.paddingleft}}px; background: {{diyitem.style.background}};" wx:elif="{{diyitem.id=='banner'}}">
                <view bind:tap="onJsEvent" data-event-type="jsUrl" data-url="{{item.linkurl}}" wx:for="{{diyitem.data}}" wx:key="{{index}}">
                    <image mode="widthFix" src="{{item.imgurl}}"></image>
                </view>
            </view>
            <view class="diy-buttons" style="margin-top: {{diyitem.style.margintop}}px; background: {{diyitem.style.backgroundcolor}};" wx:elif="{{diyitem.id=='buttons'}}">
                <view class="diy-buttons-container col-{{diyitem.params.rownum}}" wx:for="{{diyitem.data}}" wx:key="{{index}}">
                    <view bind:tap="onJsEvent" class="diy-button-col" data-event-type="jsUrl" data-url="{{item.linkurl}}" style="background: {{item.backgroundcolor}}; color: {{item.color}};">{{item.text}}</view>
                </view>
            </view>
            <view class="diy-blank" style="height: {{diyitem.style.height}}px; background: {{diyitem.style.background}};" wx:elif="{{diyitem.id=='blank'}}"></view>
            <view class="swiper-container diy-picture" style="padding: {{diyitem.style.paddingtop}}px {{diyitem.style.paddingleft}}px; background: {{diyitem.style.background}};" wx:elif="{{diyitem.id=='picture'}}">
                <swiper autoplay="true" circular="true" indicatorActiveColor="{{diyitem.style.dotbackground}}" indicatorColor="#7f7f7f" indicatorDots="true" interval="4000">
                    <swiper-item wx:for="{{diyitem.data}}" wx:key="{{index}}">
                        <view bind:tap="onJsEvent" class="swiper-slide" data-event-type="jsUrl" data-url="{{item.linkurl}}">
                            <image mode="widthFix" src="{{item.imgurl}}"></image>
                        </view>
                    </swiper-item>
                </swiper>
            </view>
            <block wx:elif="{{diyitem.id=='picturew'}}">
                <view class="diy-cube lazyload-container" style="background: {{diyitem.style.background}}; padding: {{diyitem.data_num==1?diyitem.style.paddingtop+'px '+diyitem.style.paddingleft+'px':0}};" wx:if="{{diyitem.params.row==1}}">
                    <view bind:tap="onJsEvent" data-event-type="jsUrl" data-url="{{diyitem.data[0].linkurl}}" wx:if="{{diyitem.data_num==1}}">
                        <image class="lazyload lazyload-store" src="{{diyitem.data[0].imgurl}}"></image>
                    </view>
                    <view class="diy-cube-container" wx:elif="{{diyitem.data_num>1}}">
                        <view class="diy-cube-left" style="padding: {{diyitem.style.paddingtop}}px {{diyitem.style.paddingleft}}px; padding-right:0px">
                            <view bind:tap="onJsEvent" data-event-type="jsUrl" data-url="{{diyitem.data[0].linkurl}}">
                                <image class="lazyload lazyload-store" src="{{diyitem.data[0].imgurl}}"></image>
                            </view>
                        </view>
                        <view class="diy-cube-right" style="padding: {{diyitem.data_num==2?diyitem.style.paddingtop+'px '+diyitem.style.paddingleft+'px':0}};padding-top:{{diyitem.style.paddingtop}}px">
                            <view bind:tap="onJsEvent" data-event-type="jsUrl" data-url="{{diyitem.data[1].linkurl}}" wx:if="{{diyitem.data_num==2}}">
                                <image class="lazyload lazyload-store" src="{{diyitem.data[1].imgurl}}"></image>
                            </view>
                            <block wx:elif="{{diyitem.data_num>2}}">
                                <view class="diy-cube-right1" style="padding: {{diyitem.style.paddingtop}}px {{diyitem.style.paddingleft}}px; padding-bottom:{{diyitem.style.paddingleft/2}}px;padding-top:0px;">
                                    <view bind:tap="onJsEvent" data-event-type="jsUrl" data-url="{{diyitem.data[1].linkurl}}">
                                        <image class="lazyload lazyload-store" src="{{diyitem.data[1].imgurl}}"></image>
                                    </view>
                                </view>
                                <view class="diy-cube-right2" style="padding: {{diyitem.data_num==3?diyitem.style.paddingtop+'px '+diyitem.style.paddingleft+'px':0}};padding-bottom:0px;padding-top:{{diyitem.style.paddingleft/2}}px;">
                                    <view bind:tap="onJsEvent" data-event-type="jsUrl" data-url="{{diyitem.data[2].linkurl}}" wx:if="{{diyitem.data_num==3}}">
                                        <image class="lazyload lazyload-store" src="{{diyitem.data[2].imgurl}}"></image>
                                    </view>
                                    <block wx:if="{{diyitem.data_num>3}}">
                                        <view class="left" style="padding: 0px {{diyitem.style.paddingleft}}px; padding-right: 0px;">
                                            <view bind:tap="onJsEvent" data-event-type="jsUrl" data-url="{{diyitem.data[2].linkurl}}">
                                                <image class="lazyload lazyload-store" src="{{diyitem.data[2].imgurl}}"></image>
                                            </view>
                                        </view>
                                        <view class="right" style="padding: 0px {{diyitem.style.paddingleft}}px;">
                                            <view bind:tap="onJsEvent" data-event-type="jsUrl" data-url="{{diyitem.data[3].linkurl}}">
                                                <image src="{{diyitem.data[3].imgurl}}"></image>
                                            </view>
                                        </view>
                                    </block>
                                </view>
                            </block>
                        </view>
                    </view>
                </view>
                <block wx:elif="{{diyitem.params.row>1&&diyitem.params.row<5}}">
                    <view class="diy-picturew row-{{diyitem.params.row}} lazyload-container" style="padding: {{diyitem.style.paddingtop}}px {{diyitem.style.paddingleft}}px; background: {{diyitem.style.background}};" wx:if="{{diyitem.params.showtype==0||diyitem.data_num<=diyitem.params.pagenum}}">
                        <view class="item" style="padding: {{diyitem.style.paddingtop}}px {{diyitem.style.paddingleft}}px;" wx:for="{{diyitem.data}}" wx:key="{{index}}">
                            <view bind:tap="onJsEvent" data-event-type="jsUrl" data-nocache="true" data-url="{{item.linkurl}}">
                                <image src="{{item.imgurl}}"></image>
                            </view>
                        </view>
                    </view>
                    <view class="swiper swiper-{{index}}" data-element=".swiper-{{index}}" wx:else>
                        <view class="swiper-container diy-picturew row-{{diyitem.params.row}} swiper-container-horizontal" data-autoplay="2000" data-pagination=".swiper-pagination-M1516786662941" style="padding: {{diyitem.style.paddingtop}}px {{diyitem.style.paddingleft}}px; background: {{diyitem.style.background}};">
                            <view class="swiper-wrapper" style="transform: translate3d(0px, 0px, 0px); transition-duration: 0ms;">
                                <swiper autoplay="true" circular="true" indicatorActiveColor="{{diyitem.style.dotbackground}}" indicatorColor="#7f7f7f" indicatorDots="{{diyitem.style.showdot==0?false:true}}" interval="4000" style="height:{{diyitem.style.base_height*diyitem.style.rows_num}}px">
                                    <swiper-item wx:for="{{diyitem.data}}" wx:for-item="tempitem" wx:key="{{index}}">
                                        <view class="swiper-slide swiper-slide-active">
                                            <view class="item" style="padding: {{diyitem.style.paddingtop}}px {{diyitem.style.paddingleft}}px;" wx:for="{{tempitem}}" wx:key="{{index}}">
                                                <view bind:tap="onJsEvent" data-event-type="jsUrl" data-nocache="true" data-url="{{item.linkurl}}">
                                                    <image src="{{item.imgurl}}"></image>
                                                </view>
                                            </view>
                                        </view>
                                    </swiper-item>
                                </swiper>
                            </view>
                        </view>
                    </view>
                </block>
                <view class="diy-picturew row-{{diyitem.params.row}}" style="background: {{diyitem.style.background}}" wx:elif="{{diyitem.params.row==5}}">
                    <view bind:tap="onJsEvent" class="diy-picturew-left" data-event-type="jsUrl" data-url="{{diyitem.data[0].linkurl}}" wx:if="{{diyitem.data_num>=1}}">
                        <image alt="" class="lazyload lazyload-store" src="{{diyitem.data[0].imgurl}}"></image>
                    </view>
                    <view class="diy-picturew-right" wx:if="{{diyitem.data_num>=2}}">
                        <view class="diy-picturew-right-top">
                            <view bind:tap="onJsEvent" class="left" data-event-type="jsUrl" data-url="{{diyitem.data[1].linkurl}}">
                                <image alt="" class="lazyload lazyload-store" src="{{diyitem.data[1].imgurl}}"></image>
                            </view>
                            <view bind:tap="onJsEvent" class="right" data-event-type="jsUrl" data-url="{{diyitem.data[2].linkurl}}" wx:if="{{diyitem.data_num>=3}}">
                                <image alt="" class="lazyload lazyload-store" src="{{diyitem.data[2].imgurl}}"></image>
                            </view>
                        </view>
                        <view class="diy-picturew-right-bottom" wx:if="{{diyitem.data_num>=4}}">
                            <view bind:tap="onJsEvent" class="left" data-event-type="jsUrl" data-url="{{diyitem.data[3].linkurl}}">
                                <image alt="" class="lazyload lazyload-store" src="{{diyitem.data[3].imgurl}}"></image>
                            </view>
                            <view bind:tap="onJsEvent" class="right" data-event-type="jsUrl" data-url="{{diyitem.data[4].linkurl}}" wx:if="{{diyitem.data_num>=5}}">
                                <image alt="" class="lazyload lazyload-store" src="{{diyitem.data[4].imgurl}}"></image>
                            </view>
