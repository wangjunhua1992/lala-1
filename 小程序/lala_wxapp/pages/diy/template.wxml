                                            <view wx:if="{{stores.delivery_price>0}}">配送费{{Lang.dollarSign}}{{stores.delivery_price}}</view>
                                            <view wx:else>免配送费</view>
                                        </view>
                                        <view class="item-min-delivery-right-wrap">
                                            <view class="zhunshibao" wx:if="{{stores.zhunshibao_cn}}">{{stores.zhunshibao_cn}}</view>
                                            <view class="item-min-delivery-right" style="background-color: {{diyitem.style.deliveryTitleBgcolor}}; color: {{diyitem.style.deliveryTitleColor}}" wx:if="{{stores.delivery_title}}">{{stores.delivery_title}}</view>
                                        </view>
                                    </view>
                                </navigator>
                                <view bind:tap="onToggleStoresTabDiscount" class="discount-box" data-diyindex="{{diyindex}}" data-itemindex="{{itemindex}}" data-listindex="{{listindex}}" wx:if="{{stores.activity&&(stores.activity.num>0||stores.activity.labels_num>0)}}">
                                    <view class="toggle">
                                        <view class="icon {{stores.activity.is_show_all?'icon-fold':'icon-unfold'}}" wx:if="{{diyitem.params.discountstyle==1&&stores.activity.num>2||diyitem.params.discountstyle==2&&stores.activity.labels_num>4}}"></view>
                                    </view>
                                    <block wx:if="{{diyitem.params.discountstyle==1}}" wx:for="{{stores.activity.items}}" wx:key="{{index}}">
                                        <view class="single-line" wx:if="{{index<2||stores.activity.is_show_all&&index>=2}}">
