page {
    background: #f4f4f4;
}

.container .content {
    margin: 15px;
}

.container .content .coupon-list .coupon-item {
    margin-bottom: 10px;
    height: 100px;
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
    justify-content: space-between;
}

.container .content .coupon-list .coupon-item .coupon-left {
    width: 100%;
    height: 100%;
    display: -webkit-box;
    background-color: #fff;
    position: relative;
    padding-right: 10px;
    padding-left: 13px;
}

.container .content .coupon-list .coupon-item .coupon-right {
    width: 75px;
    height: 100%;
    background-color: #FFD300;
    margin-left: 4px;
    position: relative;
    border-radius: 0px 5px 5px 0px;
}

.container .content .coupon-list .coupon-item .coupon-left .triangle-border-left {
    height: 100%;
    width: 6px;
    position: absolute;
    left: -5px;
    z-index: 1;
}

.container .content .coupon-list .coupon-item .coupon-left .triangle-border-left view {
    display: block;
    border: 2px solid transparent;
    border-right: 3px solid #fff;
    border-left: 3px solid transparent;
    width: 0px;
    height: 1px;
}

.container .content .coupon-list .coupon-item .coupon-left .img-div-backg {
    height: 75px;
    width: 75px;
    margin-top: 13px;
    background-size: 75px 75px;
}

.container .content .coupon-list .coupon-item .coupon-left .img-div-backg .img-div {
    width: 71px;
    height: 71px;
    font-size: 0px;
    margin-top: 2px;
    margin-left: 2px;
    text-align: center;
}

.container .content .coupon-list .coupon-item .coupon-left .img-div-backg .img-div image {
    width: 100%;
    height: 100%;
}

.container .content .coupon-list .coupon-item .coupon-left .pro-content {
    margin-top: 15px;
    height: 75px;
    margin-left: 10px;
    box-flex: 1;
}

.container .content .coupon-list .coupon-item .coupon-left .pro-content .pro-info {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    line-clamp: 2;
    box-orient: vertical;
    word-break: break-all;
    font-size: 13px;
    color: #232326;
    height: 28px;
    line-height: 14px;
}

.container .content .coupon-list .coupon-item .coupon-left .pro-content .pro-price {
    height: 24px;
    overflow: hidden;
    width: 100%;
    position: relative;
    display: -webkit-box;
}

.container .content .coupon-list .coupon-item .coupon-left .pro-content .pro-price .coupon-discount {
    display: inline-block;
    font-size: 16px;
    height: 20px;
    line-height: 16px;
    font-weight: bold;
    position: relative;
    color: #F95049;
    top: -2px;
}

.container .content .coupon-list .coupon-item .coupon-left .pro-content .pro-price .coupon-title {
    color: #232326;
    display: inline-block;
    box-flex: 1;
    margin-top: 12px;
    font-size: 12px;
    height: 12px;
    line-height: 12px;
    margin-left: 5px;
    text-align: left;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.container .content .coupon-list .coupon-item .coupon-left .pro-condition {
    font-size: 13px;
    color: #abacab;
    margin-top: 3px;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.container .content .coupon-list .coupon-item .coupon-right .triangle-border-right {
    height: 100%;
    width: 6px;
    position: absolute;
    left: -5px;
    z-index: 1;
}

.container .content .coupon-list .coupon-item .coupon-right .triangle-border-right .circular0 {
    position: absolute;
    height: 12px;
    width: 12px;
    border-radius: 6px 6px 6px 6px;
    background-color: #f3f5f7;
    left: -3px;
    top: -6px;
    z-index: 2;
}

.container .content .coupon-list .coupon-item .coupon-right .triangle-border-right .circular1 {
    position: absolute;
    height: 12px;
    width: 12px;
    border-radius: 6px 6px 6px 6px;
    background-color: #f3f5f7;
    left: -3px;
    bottom: -6px;
    z-index: 2;
}

.container .content .coupon-list .coupon-item .coupon-right .triangle-border-right .i {
    display: block;
    border: 2px solid transparent;
    border-right: 3px solid #FFD300;
    border-left: 3px solid #fff;
    width: 0px;
    height: 1px;
}

.container .content .coupon-list .coupon-item .coupon-right .change-block {
    width: 75px;
    height: 100%;
    margin: 0px;
    padding: 0px;
    box-sizing: border-box;
}

.container .content .coupon-list .coupon-item .coupon-right .change-block .progress-bar-block {
    width: 50px;
    margin-left: 12px;
    padding-top: 22px;
}

.container .content .coupon-list .coupon-item .coupon-right .change-block .progress-bar-block .progress-bar {
    width: 50px;
    height: 10px;
    border-radius: 5px;
    background-color: #E3BC03;
}

.container .content .coupon-list .coupon-item .coupon-right .change-block .progress-bar-block .progress-bar view {
    height: 100%;
    width: 50%;
    background-color: #fff;
    border-radius: 5px;
}

.container .content .coupon-list .coupon-item .coupon-right .change-block .progress-text {
    font-size: 12px;
    color: #fff;
    width: 100%;
    text-align: center;
    height: 10px;
    line-height: 10px;
    margin-top: 10px;
}

.container .content .coupon-list .coupon-item .coupon-right .change-block .coupon-btn {
    position: absolute;
    bottom: 12px;
    right: 6px;
    height: 18px;
    line-height: 19px;
    width: 57px;
    border: 3px solid #E3BC03;
    border-radius: 12px;
    background-color: #fff;
    font-size: 12px;
    color: #FFD300;
    text-align: center;
}

.container .content .coupon-list .coupon-item .coupon-right .change-block .coupon-btn.coupon-btn-yellow {
    background-color: #ffec00;
    color: #232326;
}

.container .content .coupon-list .coupon-item .coupon-right .change-block .had {
    width: 60px;
    height: 44px;
    display: block;
    margin-top: 0px;
    margin-left: 6px;
}

.container .content .coupon-list .coupon-item .coupon-right .change-block .had image {
    width: 100%;
    height: 100%;
}

.diy-circle {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 30px;
}

.diy-circle .circle {
    width: 50px;
    height: 50px;
    position: absolute;
    top: 50%;
    left: 50%;
    margin: -25px 0 0 -25px;
    border-radius: 50%;
    background: #fff;
}

.diy-circle .clip_left,.diy-circle .clip_right {
    width: 50px;
    height: 50px;
    position: absolute;
    top: 0px;
    left: 0px;
}

.diy-circle .circle_left,.diy-circle .circle_right {
    width: 50px;
    height: 50px;
    position: absolute;
    border-radius: 50%;
    top: 0px;
    left: 0px;
    background: #E3BC03;
}

.diy-circle .circle_right,.diy-circle .clip_right {
    clip: rect(0,auto,auto,25px);
}

.diy-circle .circle_left,.diy-circle .clip_left {
    clip: rect(0,25px,auto,0);
}

.diy-circle .mask {
    width: 40px;
    height: 40px;
    border-radius: 50%;
    left: 5px;
    top: 5px;
    background: #FFD300;
    position: absolute;
    text-align: center;
    line-height: 16px;
    font-size: 12px;
    color: #fff;
    box-sizing: border-box;
    padding-top: 5px;
    z-index: 15;
}