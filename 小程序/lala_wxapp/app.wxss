    line-height: 1.2;
    margin-bottom: 10px;
}

.navs {
    background: #a2d9f2;
}

.navs-container {
    position: relative;
    overflow: hidden;
    background-color: #fff;
    display: flex;
    flex-flow: row wrap;
}

.navs-container.col-4 .nav-col {
    width: 25%;
}

.navs-container.col-3 .nav-col {
    width: 33.33%;
}

.navs-container.col-5 .nav-col {
    width: 20%;
}

.navs-container .nav-col {
    width: 25%;
    height: auto;
    position: relative;
    padding: 0;
    margin: 8px 0;
    text-align: center;
}

.navs-container .nav-col .nav-icon {
    margin: auto;
    text-align: center;
    height: 50px;
    line-height: 50px;
}

.navs-container .nav-col image {
    display: inline-block;
    width: 50px;
    max-width: 50px;
    height: 50px;
    max-height: 100%;
}

.navs-container .nav-col .nav-text {
    font-size: 12px;
    text-align: center;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    padding-top: 4px;
}

.navs-container.radius image {
    border-radius: 10px;
}

.navs-container.circle image {
    border-radius: 100%;
}

.navs-container swiper {
    width: 100%;
    height: 80px;
}

.navs-container swiper swiper-item.one {
    width: 100%;
    display: flex;
    flew-wrap: wrap;
}

.navs-container .pointer-swiper {
    width: 100%;
    height: 80px;
}

.navs-container swiper swiper-item.not-one .nav-col {
    display: inline-block;
