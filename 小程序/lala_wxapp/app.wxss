.num-step {
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
    height: 20px;
    border: 1rpx solid #ccc;
    border-radius: 3px;
    overflow: hidden;
}

.num-step .num {
    width: 30px;
    height: 20px;
    line-height: 1;
    font-size: 12px;
    text-align: center;
    border-left: 1rpx solid #ccc;
    border-right: 1rpx solid #ccc;
}

.num-step .step-item {
    width: 20px;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 16px;
}

.num-step .step-item.disabled {
    background-color: #e8e8e8;
    color: #999;
}

.tab-group {
    width: 100%;
    position: fixed;
    top: 0px;
    z-index: 10;
    left: 0;
    right: 0;
    height: 45px;
    line-height: 45px;
    background-color: #fff;
    color: #333;
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
    justify-content: space-between;
}

.tab-group .tab-item {
    width: 25%;
    height: 100%;
    text-align: center;
    font-size: 14px;
    position: relative;
    flex: 1;
}

.tab-group .tab-item.active {
    color: #ff2d4b;
}

.tab-group .tab-item.active:after {
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 0;
    border-bottom: 2px solid #ff2d4b;
}

tab-bar~.container {
    padding-bottom: 50px;
