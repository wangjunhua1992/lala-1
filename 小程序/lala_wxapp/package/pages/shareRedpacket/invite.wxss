page {
    font-size: 14px;
    line-height: 1;
    box-sizing: border-box;
    height: 100%;
    background: url(data:image/png;base64,) repeat;
}

.shareRedpacket-invite .content .invite-img {
    padding: 20px 10px 0;
}

.shareRedpacket-invite .content .invite-img image {
    border: none;
    width: 100%;
}

.shareRedpacket-invite .content .active-day {
    position: relative;
    height: 50px;
    margin: 0 10px;
}

.shareRedpacket-invite .content .active-day .day-picture {
    position: absolute;
    left: 0;
    top: 0;
    border-radius: 100%;
    border: 2px solid #3d1e13;
    padding: 1px;
    width: 44px;
    height: 44px;
    z-index: 1;
}

.shareRedpacket-invite .content .active-day .day-picture image {
    width: 100%;
    height: 100%;
    border-radius: 100%;
    border: none;
}

.shareRedpacket-invite .content .active-day .day-info {
    margin-top: 7.4px;
    margin-bottom: 0;
    height: 32px;
    line-height: 32px;
    position: absolute;
    background: #ffee8a;
    border: 2px solid #3d1e13;
    border-radius: 6px;
    font-size: 12px;
    width: 65%;
    left: 1%;
    text-align: left;
    padding-left: 56px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.shareRedpacket-invite .content .activity-box {
    margin-top: 5px;
    padding: 0 10px;
}

.shareRedpacket-invite .content .activity-box .activity-info {
    font-size: 14px;
    padding-top: 15px;
    padding-bottom: 20px;
    color: #f00;
    background: url(data:image/png;base64,) no-repeat top center;
    background-size: 100% 100%;
    text-align: center;
}

.shareRedpacket-invite .content .activity-box .activity-info view:first-child {
    line-height: 18px;
}

.shareRedpacket-invite .content .activity-box .activity-info .packet-money {
    font-size: 16px;
}

.shareRedpacket-invite .content .activity-box .activity-info .packet-money text {
    font-size: 36px;
    line-height: 1;
}

.shareRedpacket-invite .content .activity-box .user-info {
    background: url(data:image/png;base64,) repeat-y;
    background-size: 100% auto;
    padding: 0 15px 0;
}

.shareRedpacket-invite .content .activity-box .user-info .phone-input {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
    width: 100%;
    height: 40px;
    text-align: left;
}

.shareRedpacket-invite .content .activity-box .user-info .phone-input input {
    width: 94%;
    height: 40px;
    border: none;
    padding-left: 20px;
    background: rgba(255,255,255,0);
    font-size: 14px;
}

.shareRedpacket-invite .content .activity-box .user-info .captcha {
    height: 40px;
    margin-top: 10px;
}

.clearfix:after {
    clear: both;
}

.shareRedpacket-invite .content .activity-box .user-info .captcha .captcha-input {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
    width: 65%;
    float: left;
}

.shareRedpacket-invite .content .activity-box .user-info .captcha .captcha-input input {
    width: 91%;
    height: 40px;
    border: none;
    padding-left: 20px;
    background: rgba(255,255,255,0);
    font-size: 14px;
}

.shareRedpacket-invite .content .activity-box .user-info .captcha .btn-captcha {
    display: block;
    width: 30%;
    height: 36px;
    line-height: 40px;
    border: 2px solid #3d1e13;
    border-radius: 8px;
    float: right;
}

.shareRedpacket-invite .content .activity-box .user-info .code {
    height: 40px;
    margin-top: 10px;
}

.shareRedpacket-invite .content .activity-box .user-info .code .code-input {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
    width: 65%;
    float: left;
}

.shareRedpacket-invite .content .activity-box .user-info .code .code-input input {
    width: 91%;
    height: 38px;
    border: none;
    padding-left: 20px;
    background: rgba(255,255,255,0);
    font-size: 14px;
}

.shareRedpacket-invite .content .activity-box .user-info .code .send-code {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
    display: block;
    width: 30%;
    height: 36px;
    overflow: hidden;
    line-height: 34px;
    color: #000;
    font-size: 16px;
    border: 2px solid #3d1e13;
    border-radius: 8px;
    float: right;
    text-align: center;
}

.shareRedpacket-invite .content .activity-box .user-info .now-get {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
    margin-top: 10px;
    border: 2px solid #3d1e13;
    border-radius: 8px;
    text-align: center;
}

.shareRedpacket-invite .content .activity-box .user-info .now-get view {
    display: block;
    width: 100%;
    line-height: 40px;
    color: #644505;
    font-size: 16px;
}

.shareRedpacket-invite .content .activity-box .box-bottom image {
    border: none;
    width: 100%;
}