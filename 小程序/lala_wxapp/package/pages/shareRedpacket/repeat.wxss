page {
    font-size: 14px;
    line-height: 1;
    box-sizing: border-box;
    height: 100%;
    background: url(data:image/png;base64,) repeat;
}

.shareRedpacket-repeat .content .header-img {
    padding: 20px 10px 0;
}

.shareRedpacket-repeat .content .header-img image {
    border: none;
    width: 100%;
}

.shareRedpacket-repeat .content .cannot-get {
    background: #ffee8a;
    line-height: 30px;
    width: 90%;
    font-size: 14px;
    margin: 6px auto;
    border: 2px solid #3d1e13;
    border-radius: 8px;
}

.shareRedpacket-repeat .content .cannot-get .notice {
    text-align: center;
    margin: 2px 0;
}

.shareRedpacket-repeat .content .invite {
    font-size: 14px;
    padding: 20px 0;
    color: #3d1e13;
    width: 96%;
    margin: 30px auto 0;
    overflow: hidden;
}

.shareRedpacket-repeat .content .invite .pull-left {
    width: 15%;
    -webkit-box-flex: 1;
    flex: 1;
    margin: 0 2%;
    display: block;
    border-bottom: 1px solid #333;
    float: left;
}

.shareRedpacket-repeat .content .invite view {
    width: 60%;
    color: #3d1e13;
    display: block;
    line-height: 40px;
    margin-top: -20px;
    float: left;
    text-align: center;
}

.shareRedpacket-repeat .content .invite .pull-right {
    width: 15%;
    -webkit-box-flex: 1;
    flex: 1;
    margin: 0 2%;
    display: block;
    border-bottom: 1px solid #333;
    float: right;
}

.shareRedpacket-repeat .content .invite-btn {
    font-size: 16px;
    display: block;
    background-color: #f1491e;
    border-radius: 4px;
    width: 90%;
    margin: 10px auto 0;
    line-height: 48px;
    text-align: center;
    color: #fff;
}