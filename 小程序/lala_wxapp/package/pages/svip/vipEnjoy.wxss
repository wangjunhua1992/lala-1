page {
    background-color: #F5F5F5;
    font-size: 14px;
    line-height: 1;
}

.page-vipEnjoy .title-img {
    width: 100%;
    height: 150px;
    border: none;
}

.page-vipEnjoy .title-img image {
    width: 100%;
    height: 100%;
}

.page-vipEnjoy .wrapper-list {
    display: flex;
    width: 100%;
    height: 40px;
    background: #fff;
}

.page-vipEnjoy .wrapper-list view {
    display: flex;
    flex: 1;
    align-items: center;
    justify-content: center;
    height: 100%;
}

.page-vipEnjoy .wrapper-list view .item {
    padding: 7px 0 8px 0;
    border-bottom: 2px solid transparent;
    color: #666;
    font-size: 14px;
    line-height: 16px;
    opacity: .7;
}

.page-vipEnjoy .wrapper-list view .item.active {
    border-color: #333;
    font-weight: 700;
    opacity: 1;
}

.page-vipEnjoy .goods-list {
    display: flex;
    flex-wrap: wrap;
    align-items: center;
    width: 100%;
    padding: 7.5px;
    box-sizing: border-box;
}

.page-vipEnjoy .goods-list .goods-item {
    width: 50%;
    padding: 7.5px;
    background-color: #f5f5f5;
    box-sizing: border-box;
}

.page-vipEnjoy .goods-list .goods-item .goods-img {
    width: 100%;
    height: 170px;
}

.page-vipEnjoy .goods-list .goods-item .goods-img image {
    width: 100%;
    height: 100%;
}

.page-vipEnjoy .goods-list .goods-item .goods-info {
    position: relative;
    padding: 10px;
    background-color: #fff;
}

.page-vipEnjoy .goods-list .goods-item .goods-info .goods-title {
    font-size: 15px;
    color: #333;
    font-weight: 700;
    padding-bottom: 5px;
}

.page-vipEnjoy .goods-list .goods-item .goods-info .discount {
    height: 15px;
    line-height: 15px;
    width: 55px;
    background-color: #29211b;
    color: #ffefa6;
    font-size: 12px;
    border-radius: 3px;
    text-align: center;
}

.page-vipEnjoy .goods-list .goods-item .goods-info .buy-btn {
    position: absolute;
    right: 10px;
    bottom: 10px;
    width: 30px;
    height: 30px;
    background: url(data:image/png;base64,) 0 0 no-repeat;
    background-size: 100% 100%;
}

.page-vipEnjoy .goods-list .goods-item .goods-info:after {
    position: absolute;
    content: "";
    border-top: 1px solid #eee;
    transform: scaleY(.7);
    left: 0;
    right: 0;
    bottom: 0;
    top: auto;
    border-top-style: dashed;
}

.page-vipEnjoy .goods-list .goods-item .goods-footer {
    background-color: #fff;
    padding: 5px 10px;
}