page {
    background-color: #F5F5F5;
}

.content {
    position: fixed;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    width: 100%;
    padding-bottom: 65px;
    overflow-x: hidden;
    overflow-y: scroll;
}

.content::-webkit-scrollbar {
    display: none;
}

.content .wrapper {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 2.135% 0 4%;
    width: 100%;
    background: #fff url(https://shadow.elemecdn.com/faas/h5/static/new_header_card.7c39197.png) 0 0 no-repeat;
    background-size: 100% auto;
}

.content .wrapper .card {
    display: flex;
    flex-direction: column;
    align-items: center;
    position: relative;
    padding-bottom: 5%;
    width: 82.67%;
}

.content .wrapper .card .card-top {
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 8.71%;
    line-height: 16px;
    color: #735b27;
    font-size: 14px;
}

.content .wrapper .card .card-top:before {
    display: block;
    margin: 0 10px;
    width: 50px;
    height: 1px;
    background: #735b27;
    content: "";
}

.content .wrapper .card .card-top:after {
    display: block;
    margin: 0 10px;
    width: 50px;
    height: 1px;
    background: #735b27;
    content: "";
}

.content .wrapper .card .card-top image {
    display: block;
    margin: 0 4px;
    width: 7.5px;
    height: 7.5px;
}

.content .wrapper .card .card-top text {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.content .wrapper .card .card-title {
    position: relative;
    margin-top: 5%;
    color: #624e22;
    font-weight: 700;
    font-size: 30px;
    line-height: 46px;
    width: 100%;
    white-space: nowrap;
    overflow: hidden;
    text-align: center;
}

.content .wrapper .card .card-description {
    margin-top: 6.6px;
    color: #735b28;
    font-size: 17px;
    line-height: 24px;
    width: 100%;
    white-space: nowrap;
    overflow: hidden;
    text-align: center;
}

.content .wrapper .card-list {
    position: relative;
    margin-top: 13px;
    width: 100%;
}

.content .wrapper .card-list .list-header {
    display: flex;
    align-items: center;
    justify-content: center;
}

.content .wrapper .card-list .list-header .title-header {
    color: #515151;
    font-weight: 700;
    line-height: 25px;
    font-size: 21px;
    display: flex;
    align-items: center;
    justify-content: center;
}

.content .wrapper .card-list .list-header .title-header:before {
    display: block;
    margin: 3.8px;
    width: 25.4px;
    height: 11px;
    background-image: url(data:image/png;base64,);
    background-position: 0 0;
    background-size: 100% auto;
    background-repeat: no-repeat;
    content: "";
}

.content .wrapper .card-list .list-header .title-header:after {
    display: block;
    margin: 3.8px;
    width: 25.4px;
    height: 11px;
    background-image: url(data:image/png;base64,);
    background-position: 0 0;
    background-size: 100% auto;
    background-repeat: no-repeat;
    transform: scaleX(-1);
    content: "";
}

.content .wrapper .card-list .list-bonus {
    position: absolute;
    color: #999;
    font-size: 13px;
    right: 18.7px;
    top: 5.5px;
}

.content .wrapper .card-list .list-bonus .icon-question1 {
    font-size: 15px;
    margin-left: 5px;
    color: #ccc;
}

.content .wrapper .card-list .list-item {
    display: flex;
    justify-content: space-between;
    margin: 3.24% 4.24% 0;
    padding: 2%;
    background-color: #fff;
    box-shadow: 0 0.5px 1px 0 #e9e9e9;
}

.content .wrapper .card-list .list-item view {
    display: flex;
    flex-direction: column;
    align-items: center;
    flex: 1;
}

.content .wrapper .card-list .list-item view image {
    display: block;
    width: 52.9px;
    height: 52.9px;
}

.content .wrapper .card-list .list-item view text {
    margin-top: 3px;
    color: #999;
    font-size: 12px;
    line-height: 16px;
}

.content .section {
    margin: 10px 0;
    padding: 0 7.5px;
    background: #fff;
    box-sizing: border-box;
}

.content .section .wrapper-item {
    display: flex;
    align-items: center;
    flex-direction: column;
    padding: 10px 7.5px;
}

.content .section .wrapper-item .mission-info {
    color: #535353;
    font-size: 13px;
    line-height: 18px;
    text-align: left;
    width: 100%;
}

.content .section .wrapper-item .title {
    justify-content: center;
    line-height: 48px;
}

.content .section .wrapper-item .title .icon-redpacket {
    color: #D0A651;
    font-size: 19px;
    margin-right: 10px;
}

.content .section .wrapper-item .title .icon-order {
    color: #D0A651;
    font-size: 19px;
    margin-right: 10px;
}

.content .section .wrapper-item .title text {
    color: #333;
    font-weight: 700;
    font-size: 15px;
}

.content .section .wrapper-item .description {
    color: #535353;
    font-size: 13px;
    line-height: 18px;
}

.content .section .wrapper-item .description text {
    color: #b97d2a;
}

.content .section .list .wrapper-list {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    width: 100%;
    padding: 0 7.5px;
    box-sizing: border-box;
}

.content .section .list .wrapper-list .wrapper-group {
    display: flex;
    margin-bottom: 8px;
    align-items: center;
    width: 48.7%;
    background: url(data:image/png;base64,) 0 0 no-repeat;
    background-size: 100% 100%;
}

.content .section .list .wrapper-list .amount-price {
    align-items: flex-start;
    padding-left: 7.5%;
    margin: 12.5% 0;
    width: 38%;
    color: #4a3a16;
    font-weight: 600;
}

#svip-index .content .section .list .wrapper-list .amount-price .price {
    float: left;
    font-size: 13px;
    font-style: normal;
    font-weight: 600;
}

#svip-index .content .section .list .wrapper-list .amount-price text {
    font-size: 28px;
    font-style: normal;
    font-weight: 600;
    line-height: 20px;
}

.content .section .list .wrapper-list .info {
    width: 62%;
    padding-left: 7.5%;
    color: #333;
    font-weight: 700;
    font-size: 14px;
}

.content .section .section-list {
    display: flex;
    justify-content: flex-start;
    flex-wrap: wrap;
    width: 100%;
    padding-bottom: 7.5px;
}

.content .section .section-list .section-group {
    width: 33.33%;
}

.content .section .section-list .section-group .wrapper-section-list {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin: 7.5px;
    background: url(data:image/png;base64,) 0 0 no-repeat;
    background-size: 100% 100%;
}

.content .section .section-list .section-group .wrapper-section-list .top {
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 16.39%;
}

.content .section .section-list .section-group .wrapper-section-list .top .amount {
    display: flex;
    align-items: baseline;
    color: #4a3a16;
    font-weight: 600;
}

.content .section .section-list .section-group .wrapper-section-list .top .amount .price {
    font-style: normal;
    font-size: 12px;
    font-weight: 600;
}

.content .section .section-list .section-group .wrapper-section-list .top .amount text {
    font-style: normal;
    font-size: 34px;
    font-weight: 600;
}

.content .section .section-list .section-group .wrapper-section-list .top .condition {
    margin: 20% 0;
    color: #262626;
    font-weight: 700;
    font-size: 12px;
}

.content .section .section-list .section-group .wrapper-section-list .footer {
    display: flex;
    align-items: center;
    justify-content: center;
    color: #262626;
    font-size: 12px;
    line-height: 34px;
    width: 83%;
}

.content .section .section-list .section-group .wrapper-section-list .footer text {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.content .go-member {
    display: flex;
    align-items: center;
    justify-content: center;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 65px;
    background: #fff;
}

.content .go-member .button {
    margin: 10px;
    width: 95%;
    height: 69.25%;
    border-radius: 3px;
    background: url(data:image/png;base64,) 0 0 no-repeat;
    background-size: cover;
    color: #cab96e;
    font-weight: 700;
    font-size: 18px;
    text-align: center;
}

.content .go-member .button text {
    font-weight: 700;
    line-height: 45px;
}

.content .section .wrapper-item .title .icon-discount {
    color: #D0A651;
    font-size: 17px;
    margin-right: 10px;
}

.content .discount-img {
    width: 326px;
    height: 95px;
    margin: 0 10px 5px;
}

.content .discount-img image {
    width: 100%;
    height: 100%;
}