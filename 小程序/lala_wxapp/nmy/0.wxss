.right-btn {
    position: fixed;
    z-index: 10;
    top: 100px;
    right: 15px;
    display: flex;
    flex-flow: column nowrap;
    align-items: center;
    justify-content: flex-start;
}

.right-btn .btn-item {
    width: 42px;
    height: 42px;
    border-radius: 50%;
    background: rgba(255,45,75,0.6);
    color: #fff;
    font-size: 12px;
    display: flex;
    flex-flow: column wrap;
    align-items: center;
    justify-content: center;
    line-height: 1.2;
    margin-bottom: 10px;
}

.navs {
    background: #a2d9f2;
}

.navs-container {
    position: relative;
    overflow: hidden;
    background-color: #fff;
    display: flex;
    flex-flow: row wrap;
}

.navs-container.col-4 .nav-col {
    width: 25%;
}

.navs-container.col-3 .nav-col {
    width: 33.33%;
}

.navs-container.col-5 .nav-col {
    width: 20%;
}

.navs-container .nav-col {
    width: 25%;
    height: auto;
    position: relative;
    padding: 0;
    margin: 8px 0;
    text-align: center;
}

.navs-container .nav-col .nav-icon {
    margin: auto;
    text-align: center;
    height: 50px;
    line-height: 50px;
}

.navs-container .nav-col image {
    display: inline-block;
    width: 50px;
    max-width: 50px;
    height: 50px;
    max-height: 100%;
}

.navs-container .nav-col .nav-text {
    font-size: 12px;
    text-align: center;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    padding-top: 4px;
}

.navs-container.radius image {
    border-radius: 10px;
}

.navs-container.circle image {
    border-radius: 100%;
}

.navs-container swiper {
    width: 100%;
    height: 80px;
}

.navs-container swiper swiper-item.one {
    width: 100%;
    display: flex;
    flew-wrap: wrap;
}

.navs-container .pointer-swiper {
    width: 100%;
    height: 80px;
}

.navs-container swiper swiper-item.not-one .nav-col {
    display: inline-block;
}

.navs-container swiper.snot-one {
    width: 100%;
    height: 165px;
}

.num-step {
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
    height: 20px;
    border: 1rpx solid #ccc;
    border-radius: 3px;
    overflow: hidden;
}

.num-step .num {
    width: 30px;
    height: 20px;
    line-height: 1;
    font-size: 12px;
    text-align: center;
    border-left: 1rpx solid #ccc;
    border-right: 1rpx solid #ccc;
}

.num-step .step-item {
    width: 20px;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 16px;
}

.num-step .step-item.disabled {
    background-color: #e8e8e8;
    color: #999;
}

.tab-group {
    width: 100%;
    position: fixed;
    top: 0px;
    z-index: 10;
    left: 0;
    right: 0;
    height: 45px;
    line-height: 45px;
    background-color: #fff;
    color: #333;
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
    justify-content: space-between;
}

.tab-group .tab-item {
    width: 25%;
    height: 100%;
    text-align: center;
    font-size: 14px;
    position: relative;
    flex: 1;
}

.tab-group .tab-item.active {
    color: #ff2d4b;
}

.tab-group .tab-item.active:after {
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 0;
    border-bottom: 2px solid #ff2d4b;
}