page {
    background-color: #f5f5f5;
    font-size: 15px;
}

.page-member-favorite {
    line-height: 1;
}

.page-member-favorite .favorite-item {
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
    justify-content: space-between;
    margin: 15px;
    background-color: #fff;
    border-radius: 5px;
    padding: 15px;
}

.page-member-favorite .favorite-item .favorite-img {
    position: relative;
    width: 80px;
    height: 80px;
    margin-right: 10px;
}

.page-member-favorite .favorite-item .favorite-img image {
    width: 100%;
    height: 100%;
}

.page-member-favorite .favorite-item .favorite-img .mark {
    position: absolute;
    width: 44px;
    height: 28px;
    top: 0;
    left: 10px;
    padding-top: 4px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
    color: #fff;
    font-size: 12px;
    text-align: center;
}

.page-member-favorite .favorite-item .favorite-info {
    flex: 1;
    height: 80px;
    display: flex;
    flex-flow: column nowrap;
    align-items: flex-start;
    justify-content: space-between;
    width: 100%;
}

.page-member-favorite .favorite-item .favorite-info .store {
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
    justify-content: space-between;
    width: 100%;
}

.page-member-favorite .favorite-item .favorite-info .store .name {
    font-size: 16px;
    font-weight: 600;
}

.page-member-favorite .favorite-item .favorite-info .store .order-type {
    display: inline-block;
    background-color: #ff2d4b;
    color: #fff;
    font-size: 12px;
    line-height: 12px;
    padding: 3px 5px;
    border-radius: 3px;
    margin-right: 5px;
}

.page-member-favorite .favorite-item .favorite-info .label {
    color: #fff;
}

.page-member-favorite .favorite-item .favorite-info .label.label-info {
    background-color: #5bc0de;
}

.page-member-favorite .favorite-item .favorite-info .label.label-danger {
    background-color: #ff2d4b;
}

.page-member-favorite .favorite-item .favorite-info .label.label-warning {
    background-color: #f0ad4e;
}

.page-member-favorite .favorite-item .favorite-info .goods {
    font-size: 14px;
    color: #333;
}

.page-member-favorite .favorite-item .favorite-info .price {
    display: flex;
    align-items: center;
}

.page-member-favorite .favorite-item .favorite-info .price .new-price {
    font-size: 16px;
    color: #ff2d4b;
    font-weight: bold;
}

.page-member-favorite .favorite-item .favorite-info .price .old-price {
    font-size: 14px;
    margin-left: 5px;
    color: #999;
    text-decoration: line-through;
}