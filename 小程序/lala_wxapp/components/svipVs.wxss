.svip-vs {
    padding: 20px 15px;
    background-color: #fff;
    font-size: 15px;
    line-height: 1;
}

.svip-vs .popup-svip-title {
    display: flex;
    align-items: center;
    font-weight: 700;
    white-space: nowrap;
    padding-bottom: 15px;
    text-align: center;
}

.svip-vs .popup-svip-title:before,.svip-vs .popup-svip-title:after {
    display: block;
    margin: 0 3.5px;
    width: 23px;
    height: 10px;
    background-image: url(data:image/png;base64,);
    background-position: 0 0;
    background-size: 100% auto;
    background-repeat: no-repeat;
    content: "";
}

.svip-vs .popup-svip-title:after {
    transform: scaleX(-1);
}

.svip-vs .svip-img {
    display: flex;
    width: 100%;
    height: 73px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% auto;
    margin-bottom: 15px;
}

.svip-vs .svip-img .svip-vs-left {
    width: 50%;
    display: flex;
    flex-flow: column nowrap;
    align-items: center;
    justify-content: center;
    color: #FFE98B;
}

.svip-vs .svip-img .svip-vs-left .icon {
    font-size: 12px;
    padding-bottom: 3px;
}

.svip-vs .svip-img .svip-vs-left .svip-vs-price {
    font-size: 26px;
    padding-top: 3px;
}

.svip-vs .svip-img .svip-vs-right {
    width: 50%;
    display: flex;
    flex-flow: column nowrap;
    align-items: center;
    justify-content: center;
    color: #656565;
}

.svip-vs .svip-img .svip-vs-right .svip-putong-title {
    font-size: 12px;
}

.svip-vs .svip-img .svip-vs-right .svip-putong-price {
    font-size: 26px;
    padding-top: 5px;
}

.svip-vs .buy-svip-text {
    font-size: 14px;
    color: #999;
    text-align: center;
}

.svip-vs .svip-price-btn {
    display: flex;
    flex-flow: column nowrap;
    align-items: center;
    justify-content: center;
    line-height: 1;
    width: 100%;
    height: 45px;
    text-align: center;
    background: linear-gradient(90deg,#faeaa7,#e0c470);
    border: none;
    border-radius: 3px;
    padding: 0;
    color: #7C4F0B;
}

.svip-vs .price-btn {
    min-height: 45px;
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    text-align: center;
    color: #7c7c7c;
    font-weight: 600;
}

.svip-vs .van-button::before {
    border-color: transparent;
    background-color: transparent;
}