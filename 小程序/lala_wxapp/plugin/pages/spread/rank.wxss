page {
    background: #f4f4f4;
}

.page-rank .banner {
    z-index: 1;
    width: 100%;
    position: fixed;
    top: 0;
}

.page-rank .banner .banner-image {
    position: absolute;
    width: 100%;
    height: 130px;
    z-index: -10;
}

.page-index .banner .banner-row {
    padding-top: 30px;
}

.page-rank .banner .banner-avtar {
    padding-top: 10px;
    text-align: center;
    z-index: 100;
}

.page-rank .banner .banner-avtar .userphoto {
    width: 61px;
    height: 61px;
    border-radius: 100%;
}

.page-rank .banner .banner-row {
    z-index: 100;
}

.page-rank .banner .banner-row .col-33 .titles {
    color: #fff;
    font-size: 14px;
}

.page-rank .banner .banner-row .col-33 {
    text-align: center;
}

.page-rank .banner .banner-row .col-33 .days {
    color: #fff;
    font-size: 18px;
}

.page-rank .banner .banner-row .username {
    position: relative;
    top: -28px;
}

.page-rank .scorll {
    height: 100%;
    padding-top: 130px;
}

.list-title {
    width: 95%;
    margin: 10px 0px 0px 10px;
    background: #fff;
    font-size: 15px;
    line-height: 55px;
    text-align: center;
    height: 55px;
    border-radius: 8px 8px 0px 0px;
}

.list-title .col:first-child {
    width: 20%;
    text-align: center;
}

.list-title .col {
    text-align: left;
    float: left;
    width: 57%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.list-title .col-num {
    line-height: 55px;
    text-align: center;
}

.list-title .col:last-child {
    width: 23%;
}

.list-group .col .sort-img {
    width: 33.11px;
    height: 33.11px;
}

.list-group .col .user-img {
    position: relative;
    top: 6px;
    width: 44.16px;
    height: 44.16px;
    border-radius: 100%;
}

.list-group .col .username {
    position: relative;
    top: -10px;
    left: 5px;
    display: inline-block;
}

.list-group .col {
    display: inline-block;
    text-align: left;
    height: 55px;
    line-height: 55px;
}

.list-group .col .color-danger {
    width: 24%;
    float: left;
    line-height: 55px;
    text-align: left;
}

.list-group {
    float: left;
    margin: 0px 0px 0px 10px;
    background: #F5F5F5;
    line-height: 55px;
    text-align: center;
    border-radius: 0px;
}

.list-group .col:first-child {
    width: 19%;
    padding-top: 5px;
    text-align: center;
}

.list-group .col:last-child {
    text-align: center;
}

.list-group:nth-child(odd) {
    background: #fff;
}