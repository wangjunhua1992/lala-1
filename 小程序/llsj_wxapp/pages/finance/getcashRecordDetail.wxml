<view class="page-getcashDetail">
    <view class="content border-1px-b">
        <view class="weui-form-preview__hd">
            <view class="weui-form-preview__item">
                <view class="weui-form-preview__label zan-font-18">提现金额</view>
                <view class="weui-form-preview__value_in-hd {{getcashDetail.status==1?'color-green':'color-red'}}"> {{getcashDetail.get_fee}} </view>
            </view>
        </view>
        <view class="weui-form-preview__bd">
            <view class="weui-form-preview__item">
                <view class="weui-form-preview__label zan-font-16">订单号</view>
                <view class="weui-form-preview__value">{{getcashDetail.trade_no}}</view>
            </view>
            <view class="weui-form-preview__item">
                <view class="weui-form-preview__label zan-font-16">提现金额</view>
                <view class="weui-form-preview__value">{{getcashDetail.get_fee}}</view>
            </view>
            <view class="weui-form-preview__item">
                <view class="weui-form-preview__label zan-font-16">手续费</view>
                <view class="weui-form-preview__value">{{getcashDetail.take_fee}}</view>
            </view>
            <view class="weui-form-preview__item">
                <view class="weui-form-preview__label zan-font-16">实际到账</view>
                <view class="weui-form-preview__value">{{getcashDetail.final_fee}}</view>
            </view>
            <view class="weui-form-preview__item">
                <view class="weui-form-preview__label zan-font-16">申请时间</view>
                <view class="weui-form-preview__value">{{getcashDetail.addtime}}</view>
            </view>
            <view class="weui-form-preview__item">
                <view class="weui-form-preview__label zan-font-16">交心状态</view>
                <view class="weui-form-preview__value">{{getcashDetail.status_cn}}</view>
            </view>
        </view>
    </view>
</view>
