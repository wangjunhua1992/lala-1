page {
    background-color: #f5f5f5;
    font-size: 14px;
}

.page-select .zan-cell.shop-item {
    padding: 10px 15px;
}

.page-select .shop-item .zan-cell_title {
    display: flex;
    justify-content: space-between;
    flex-flow: row nowrap;
    align-items: center;
    max-width: 95%;
}

.page-select .shop-item .zan-cell_title .logo {
    width: 40px;
    height: 40px;
    font-size: 0;
    margin-right: 10px;
}

.page-select .shop-item .zan-cell_title .logo image {
    width: 100%;
    height: 100%;
    border-radius: 5px;
}

.page-select .shop-item .zan-cell_title .title {
    width: 85%;
    font-size: 15px;
}